{
  "name": "ccp2-solo-mvp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./AngularServer/server.js",
    "postinstall": "ng build",
    "hack:start": "ng serve --proxy-config proxy.config.json",
    "hack:build": "ng build",
    "hack:watch": "ng build --watch --configuration development",
    "hack:test": "ng test",
    "hack:api": "nodemon ./AngularServer/server.js",
    "migrate": "npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "reset": "npx prisma migrate reset --skip-seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "engines": {
    "node": "14.17.5",
    "npm": "6.14.14"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.11",
    "@angular/cli": "~12.2.10",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.11",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@prisma/client": "^3.3.0",
    "@types/node": "^12.11.1",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "rxjs": "~6.6.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.0",
    "typescript": "~4.3.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.10",
    "@types/jasmine": "~3.8.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "nodemon": "^2.0.14",
    "prisma": "^3.3.0"
  }
}
